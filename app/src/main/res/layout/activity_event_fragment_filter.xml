<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".event_fragment_filter">

    <Button
        android:id="@+id/resetFilterButton"
        style="@style/Widget.AppCompat.Button.Borderless"
        android:layout_width="86dp"
        android:layout_height="48dp"
        android:layout_marginTop="8dp"
        android:text="Reset"
        android:textAllCaps="false"
        android:textColor="#1072BA"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/events_text2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/events_text2"
        app:layout_constraintTop_toTopOf="@+id/events_text2" />

    <ToggleButton
        android:id="@+id/AthleticsB"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginTop="16dp"
        android:background="#FFFFFF"
        android:textColor="#1072BA"
        android:textOn="Athletics"
        android:textOff="Athletics"
        android:textSize="10sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/AdmissionB" />

    <ToggleButton
        android:id="@+id/FacultyStaffB"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginTop="16dp"
        android:background="#FFFFFF"
        android:textColor="#1072BA"
        android:textOn="Faculty &amp; Staff"
        android:textOff="Faculty &amp; Staff"
        android:textSize="10sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/AthleticsB" />

    <ToggleButton
        android:id="@+id/LGBTQB"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginTop="16dp"
        android:background="#FFFFFF"
        android:textColor="#1072BA"
        android:textOff="LGBTQ+"
        android:textOn="LGBTQ+"
        android:textSize="10sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.501"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/FacultyStaffB" />

    <ToggleButton
        android:id="@+id/STEMB"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginTop="16dp"
        android:background="#FFFFFF"
        android:textColor="#1072BA"
        android:textOn="STEM"
        android:textOff="STEM"
        android:textSize="10sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.501"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/LGBTQB" />

    <ToggleButton
        android:id="@+id/CareerB"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginStart="24dp"
        android:layout_marginLeft="24dp"
        android:background="#FFFFFF"
        android:textColor="#1072BA"
        android:textOn="Career"
        android:textOff="Career"
        android:textSize="10sp"
        app:layout_constraintBottom_toBottomOf="@+id/AthleticsB"
        app:layout_constraintStart_toEndOf="@+id/AthleticsB"
        app:layout_constraintTop_toTopOf="@+id/AthleticsB" />

    <ToggleButton
        android:id="@+id/GlobalB"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginStart="24dp"
        android:layout_marginLeft="24dp"
        android:background="#FFFFFF"
        android:textColor="#1072BA"
        android:textOn="Global"
        android:textOff="Global"
        android:textSize="10sp"
        app:layout_constraintBottom_toBottomOf="@+id/FacultyStaffB"
        app:layout_constraintStart_toEndOf="@+id/FacultyStaffB"
        app:layout_constraintTop_toTopOf="@+id/FacultyStaffB"
        app:layout_constraintVertical_bias="0.0" />

    <ToggleButton
        android:id="@+id/LibraryB"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginStart="24dp"
        android:layout_marginLeft="24dp"
        android:background="#FFFFFF"
        android:textColor="#1072BA"
        android:textOn="Library"
        android:textOff="Library"
        android:textSize="10sp"
        app:layout_constraintBottom_toBottomOf="@+id/LGBTQB"
        app:layout_constraintStart_toEndOf="@+id/LGBTQB"
        app:layout_constraintTop_toTopOf="@+id/LGBTQB" />

    <ToggleButton
        android:id="@+id/StudentsB"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginStart="24dp"
        android:layout_marginLeft="24dp"
        android:background="#FFFFFF"
        android:textColor="#1072BA"
        android:textOn="Students"
        android:textOff="Students"
        android:textSize="10sp"
        app:layout_constraintBottom_toBottomOf="@+id/STEMB"
        app:layout_constraintStart_toEndOf="@+id/STEMB"
        app:layout_constraintTop_toTopOf="@+id/STEMB"
        app:layout_constraintVertical_bias="0.0" />

    <ToggleButton
        android:id="@+id/ArtsB"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginEnd="24dp"
        android:layout_marginRight="24dp"
        android:background="#FFFFFF"
        android:textColor="#1072BA"
        android:textOn="Arts"
        android:textOff="Arts"
        android:textSize="10sp"
        app:layout_constraintBottom_toBottomOf="@+id/AthleticsB"
        app:layout_constraintEnd_toStartOf="@+id/AthleticsB"
        app:layout_constraintTop_toTopOf="@+id/AthleticsB" />

    <ToggleButton
        android:id="@+id/DiversityB"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginEnd="24dp"
        android:layout_marginRight="24dp"
        android:background="#FFFFFF"
        android:textColor="#1072BA"
        android:textOff="Diversity"
        android:textOn="Diversity"
        android:textSize="10sp"
        app:layout_constraintBottom_toBottomOf="@+id/FacultyStaffB"
        app:layout_constraintEnd_toStartOf="@+id/FacultyStaffB"
        app:layout_constraintTop_toTopOf="@+id/FacultyStaffB"
        app:layout_constraintVertical_bias="0.0" />

    <ToggleButton
        android:id="@+id/HealthWellnessB"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginEnd="24dp"
        android:layout_marginRight="24dp"
        android:background="#FFFFFF"
        android:textColor="#1072BA"
        android:textOff="Health &amp; Wellness"
        android:textOn="Health &amp; Wellness"
        android:textSize="10sp"
        app:layout_constraintBottom_toBottomOf="@+id/LGBTQB"
        app:layout_constraintEnd_toStartOf="@+id/LGBTQB"
        app:layout_constraintTop_toTopOf="@+id/LGBTQB" />

    <ToggleButton
        android:id="@+id/MusicB"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginEnd="24dp"
        android:layout_marginRight="24dp"
        android:background="#FFFFFF"
        android:textColor="#1072BA"
        android:textOn="Music"
        android:textOff="Music"
        android:textSize="10sp"
        app:layout_constraintBottom_toBottomOf="@+id/STEMB"
        app:layout_constraintEnd_toStartOf="@+id/STEMB"
        app:layout_constraintTop_toTopOf="@+id/STEMB" />

    <TextView
        android:id="@+id/events_text2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Events"
        android:textColor="#1072BA"
        android:textSize="45sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Categories"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/events_text2" />

    <ToggleButton
        android:id="@+id/AdmissionB"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginTop="16dp"
        android:background="#FFFFFF"
        android:textColor="#1072BA"
        android:textOn="Admission"
        android:textOff="Admission"
        android:textSize="10sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <ToggleButton
        android:id="@+id/AlumniB"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginStart="24dp"
        android:layout_marginLeft="24dp"
        android:background="#FFFFFF"
        android:textColor="#1072BA"
        android:textOn="Alumni"
        android:textOff="Alumni"
        android:textSize="10sp"
        app:layout_constraintBottom_toBottomOf="@+id/AdmissionB"
        app:layout_constraintStart_toEndOf="@+id/AdmissionB"
        app:layout_constraintTop_toTopOf="@+id/AdmissionB"
        app:layout_constraintVertical_bias="0.0" />

    <ToggleButton
        android:id="@+id/AcademicB"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginEnd="24dp"
        android:layout_marginRight="24dp"
        android:background="#FFFFFF"
        android:textColor="#1072BA"
        android:textOff="Academic"
        android:textOn="Academic"
        android:textSize="10sp"
        app:layout_constraintBottom_toBottomOf="@+id/AdmissionB"
        app:layout_constraintEnd_toStartOf="@+id/AdmissionB"
        app:layout_constraintTop_toTopOf="@+id/AdmissionB" />

</androidx.constraintlayout.widget.ConstraintLayout>