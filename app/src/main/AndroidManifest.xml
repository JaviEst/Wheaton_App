<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.wheaton">
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/wheatonlogo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/wheatonlogo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".academic_calendar"></activity>
        <activity android:name=".imp_phone_num" />
        <activity android:name=".financial_aid_office" />
        <activity android:name=".admissions_office" />
        <activity android:name=".registrar_office" />
        <activity android:name=".clubs" />
        <activity android:name=".education" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.VIEW" />

                <data android:scheme="http" android:host="www.example.com" />
                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>